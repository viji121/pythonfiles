#addition of diagonal elements:
a=[
   [1,2,3],
   [4,5,6],
   [7,8,9],
           ]
rows=len(a)
cols=len(a[0])
principal=0
secondary=0
for i in range(0,rows):
    for j in range(0,cols):
        if(i == j):
            principal += a[i][j]
            if((i+j)==(rows-1)):
                secondary+=a[i][j]
                print("principal diagonal",principal)
                print("secondary diagonal",secondary)

